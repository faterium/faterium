{
	"name": "@faterium/app",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev --port 3000",
		"build": "vite build",
		"package": "vite package",
		"preview": "vite preview",
		"sync": "svelte-kit sync",
		"prepare": "run-s sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "run-s check lint:svelte",
		"lint:svelte": "eslint \"src\" && prettier --check --ignore-path=../../.prettierignore .",
		"format": "prettier --write --ignore-path=../../.prettierignore .",
		"clean": "rm -rf .turbo .svelte-kit node_modules"
	},
	"devDependencies": {
		"@faterium/config": "workspace:*",
		"@sveltejs/adapter-cloudflare": "^2.2.2",
		"@sveltejs/kit": "latest",
		"autoprefixer": "^10.4.14",
		"esbuild": "^0.17.18",
		"lint-staged": "^13.2.1",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.23",
		"svelte": "^3.58.0",
		"svelte-check": "^2.10.3",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.3.1",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.3.1"
	}
}
